                    <IsEuropeContext/>
                    <IfValidDate startRef="EU_BETA_START" endRef="EU_BETA_END"/>
                    <IfTutorialCompleted/>
                    <IfHasSeenPopup uniquename="BETA_CHALLENGE" frequency="Hours" timefactor="18" operation="less" amount="1" targetfilter="character"/>

{
  "?xml": {
    "@version": "1.0",
    "@encoding": "utf-8"
  },
  "eventbasedpopups": {
    "@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance",
    "@xsi:noNamespaceSchemaLocation": "eventbasedpopups.xsd",
    "frequencyrules": {
      "frequencyrule": [
        {
          "@uniquename": "NONE_PREMIUM",
          "timeframes": {
            "timeframe": [
              {
                "@range": "1-2",
                "@minutes": "15"
              },
              {
                "@range": "2-3",
                "@minutes": "30"
              },
              {
                "@range": "3-4",
                "@minutes": "45"
              }
            ]
          },
          "conditions": {
            "and": {
              "not": {
                "IfHasPremium": null
              }
            }
          }
        },
        {
          "@uniquename": "SMALL_PREMIUM",
          "timeframes": {
            "timeframe": {
              "@range": "1-2",
              "@minutes": "15"
            }
          },
          "conditions": {
            "and": {
              "IfHasPremium": null,
              "IfHasPremiumDays": {
                "@operation": "less",
                "@value": "7"
              },
              "not": {
                "IfHasRecurringSubscription": null
              }
            }
          }
        },
        {
          "@uniquename": "BIG_PREMIUM",
          "timeframes": {
            "timeframe": {
              "@range": "1-2",
              "@minutes": "15"
            }
          },
          "conditions": {
            "or": {
              "IfHasPremiumDays": {
                "@operation": "greaterOrEqual",
                "@value": "7"
              },
              "IfHasRecurringSubscription": null
            }
          }
        }
      ]
    },
    "popupqueue": {
      "allowedtriggers": {
        "trigger": {
          "@action": "GameLogin",
          "@totalpopups": "4"
        }
      },
      "allowedcategories": {
        "category": [
          {
            "@name": "Announcements",
            "@totalpopups": "1"
          },
          {
            "@name": "Advertisement",
            "@totalpopups": "1"
          },
          {
            "@name": "GameFeature",
            "@totalpopups": "1"
          },
          {
            "@name": "ActivityHUB",
            "@totalpopups": "1"
          },
          {
            "@name": "Mobile",
            "@totalpopups": "1"
          }
        ]
      }
    },
    "popups": {
      "popup": [
        {
          "@uniquename": "GUEST_ACCOUNT_INCENTIVIZATION",
          "@priority": "1",
          "@type": "GuestAccount",
          "categories": {
            "category": [
              {
                "@name": "GuestAccount"
              },
              {
                "@name": "Advertisement"
              }
            ]
          },
          "triggers": {
            "trigger": [
              {
                "@action": "OpenFastTravelUi"
              },
              {
                "@action": "GameLogin"
              }
            ]
          },
          "conditions": {
            "or": {
              "and": [
                {
                  "IfIsGuestAccount": null,
                  "IfTriggeredThrough": {
                    "@action": "OpenFastTravelUi"
                  },
                  "IfHasSeenPopup": {
                    "@uniquename": "GUEST_ACCOUNT_INCENTIVIZATION",
                    "@frequency": "AllTime",
                    "@operation": "lessOrEqual",
                    "@amount": "1"
                  }
                },
                {
                  "not": {
                    "IfTutorialCompleted": null
                  },
                  "IfIsGuestAccount": null,
                  "IfGuestLoginCount": {
                    "@operation": "equals",
                    "@value": "2"
                  },
                  "IfHasSeenPopup": {
                    "@uniquename": "GUEST_ACCOUNT_INCENTIVIZATION",
                    "@frequency": "AllTime",
                    "@operation": "equals",
                    "@amount": "0"
                  }
                },
                {
                  "IfIsGuestAccount": null,
                  "IfTutorialCompleted": null,
                  "IfHasSeenPopup": [
                    {
                      "@uniquename": "GUEST_ACCOUNT_INCENTIVIZATION",
                      "@frequency": "AllTime",
                      "@operation": "greaterOrEqual",
                      "@amount": "1"
                    },
                    {
                      "@uniquename": "GUEST_ACCOUNT_INCENTIVIZATION",
                      "@frequency": "Hours",
                      "@timefactor": "12",
                      "@operation": "equals",
                      "@amount": "0"
                    }
                  ]
                }
              ]
            }
          }
        },
        {
          "@uniquename": "SEASON_WINNER",
          "@priority": "4",
          "@type": "SeasonWinner",
          "categories": {
            "category": [
              {
                "@name": "SeasonWinner"
              },
              {
                "@name": "GameFeature"
              }
            ]
          },
          "triggers": {
            "trigger": {
              "@action": "GameLogin"
            }
          },
          "conditions": {
            "and": {
              "IfTutorialCompleted": null,
              "not": {
                "IfHasSeenSeasonWinnerPopup": {
                  "@uniquename": "SEASON_WINNER"
                }
              }
            }
          }
        },
        {
          "@uniquename": "ACTIVITY_HUB",
          "@priority": "1",
          "@type": "ActivityHUB",
          "categories": {
            "category": {
              "@name": "ActivityHUB"
            }
          },
          "triggers": {
            "trigger": {
              "@action": "GameLogin"
            }
          },
          "conditions": {
            "and": {
              "IfTutorialCompleted": null,
              "IfEnabledActivityPopupOnLogin": null,
              "IfHasSeenPopup": {
                "@uniquename": "ACTIVITY_HUB",
                "@frequency": "Days",
                "@timefactor": "1",
                "@operation": "less",
                "@amount": "1",
                "@targetfilter": "character"
              }
            }
          }
        },
        {
          "@uniquename": "FACTION_WARFARE_UNLOCK",
          "@priority": "7",
          "@type": "FactionWarfareUnlock",
          "categories": {
            "category": [
              {
                "@name": "FactionWarfare"
              },
              {
                "@name": "GameFeature"
              }
            ]
          },
          "triggers": {
            "trigger": {
              "@action": "GameLogin"
            }
          },
          "conditions": {
            "and": {
              "IfHasUnlockedFactionWarfare": null,
              "IfHasSeenPopup": {
                "@uniquename": "FACTION_WARFARE_UNLOCK",
                "@frequency": "AllTime",
                "@operation": "equals",
                "@amount": "0",
                "@targetfilter": "character"
              }
            }
          }
        },
        {
          "@uniquename": "MOBILE_STORE_RATING",
          "@priority": "9",
          "@type": "MobileStoreRating",
          "categories": {
            "category": [
              {
                "@name": "MobileStoreRating"
              },
              {
                "@name": "Mobile"
              }
            ]
          },
          "triggers": {
            "trigger": [
              {
                "@action": "GameLogin"
              },
              {
                "@action": "ClusterChange"
              }
            ]
          },
          "conditions": {
            "and": {
              "IfOnPlatform": {
                "@platform": "MobileStore"
              },
              "IfTutorialCompleted": null,
              "IfCharacterOnlineHours": {
                "@operation": "greaterOrEqual",
                "@value": "5"
              },
              "IfHasSeenPopup": {
                "@uniquename": "MOBILE_STORE_RATING",
                "@frequency": "Days",
                "@timefactor": "7",
                "@operation": "equals",
                "@amount": "0"
              }
            }
          }
        },
        {
          "@uniquename": "PORTAL_CITY_INFO",
          "@priority": "10",
          "@type": "GenericTutorial",
          "categories": {
            "category": [
              {
                "@name": "GenericTutorial"
              },
              {
                "@name": "CityPortal"
              }
            ]
          },
          "triggers": {
            "trigger": {
              "@action": "ClusterChange"
            }
          },
          "conditions": {
            "and": {
              "IfInCityType": {
                "@citytype": "PortalCity"
              }
            }
          }
        },
        {
          "@uniquename": "GUILD_FINDER_INFO",
          "@priority": "10",
          "@type": "GuildFinder",
          "@title": "@EVENT_BASED_POPUPS_GUILD_FINDER_TITLE",
          "@description": "@EVENT_BASED_POPUPS_GUILD_FINDER_DESCRIPTION",
          "@imagepath": "Gui/EventBasedPopups/EVENT_POPUP_GUILD_FINDER",
          "categories": {
            "category": [
              {
                "@name": "GameFeature"
              },
              {
                "@name": "Advertisement"
              }
            ]
          },
          "triggers": {
            "trigger": {
              "@action": "ClusterChange"
            }
          },
          "conditions": {
            "and": {
              "not": {
                "IfIsInGuild": null
              },
              "IfTutorialCompleted": null,
              "IfAccountAgeDays": {
                "@value": "30",
                "@operation": "lessOrEqual"
              },
              "IfHasSeenPopup": {
                "@uniquename": "GUILD_FINDER_INFO",
                "@frequency": "Days",
                "@operation": "equals",
                "@timefactor": "7",
                "@amount": "0"
              }
            }
          }
        },
        {
          "@uniquename": "GUILD_FINDER_VETERAN_INFO",
          "@priority": "10",
          "@type": "GuildFinder",
          "@title": "@EVENT_BASED_POPUPS_GUILD_FINDER_VETERAN_TITLE",
          "@description": "@EVENT_BASED_POPUPS_GUILD_FINDER_VETERAN_DESCRIPTION",
          "@imagepath": "Gui/EventBasedPopups/EVENT_POPUP_GUILD_FINDER",
          "categories": {
            "category": [
              {
                "@name": "GameFeature"
              },
              {
                "@name": "Advertisement"
              }
            ]
          },
          "triggers": {
            "trigger": {
              "@action": "ClusterChange"
            }
          },
          "conditions": {
            "and": {
              "not": {
                "IfIsInGuild": null
              },
              "IfTutorialCompleted": null,
              "IfAccountAgeDays": {
                "@value": "30",
                "@operation": "greater"
              },
              "IfHasSeenPopup": [
                {
                  "@uniquename": "GUILD_FINDER_VETERAN_INFO",
                  "@frequency": "Days",
                  "@operation": "equals",
                  "@timefactor": "28",
                  "@amount": "0"
                },
                {
                  "@uniquename": "GUILD_FINDER_INFO",
                  "@frequency": "Days",
                  "@operation": "equals",
                  "@timefactor": "7",
                  "@amount": "0"
                }
              ]
            }
          }
        },
        {
          "@uniquename": "BETA_WELCOME",
          "@priority": "100",
          "@type": "GenericTutorial",
          "categories": {
            "category": [
              {
                "@name": "GenericTutorial"
              },
              {
                "@name": "Announcements"
              }
            ]
          },
          "triggers": {
            "trigger": [
              {
                "@action": "GameLogin"
              },
              {
                "@action": "ClusterChange"
              }
            ]
          },
          "conditions": {
            "and": {
              "IsAsiaContext": null,
              "IfValidDate": {
                "@start": "2023-02-20T10:00:00Z",
                "@end": "2023-03-13T10:00:00Z"
              },
              "IfHasSeenPopup": {
                "@uniquename": "BETA_WELCOME",
                "@frequency": "AllTime",
                "@operation": "equals",
                "@amount": "0",
                "@targetfilter": "character"
              }
            }
          }
        }
      ]
    }
  }
}